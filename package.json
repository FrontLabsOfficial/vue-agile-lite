{
  "name": "vue-agile-lite",
  "version": "1.0.0",
  "description": "A carousel component for Vue.js",
  "author": "ErikPham <phucpm.it@gmail.com>",
  "scripts": {
    "serve": "vite -c playground/vite.config.js",
    "release": "node scripts/release.js",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 1"
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "node scripts/verifyCommit.js"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "eslint --quiet"
    ],
    "*.ts?(x)": [
      "eslint --quiet"
    ]
  },
  "main": "src/index.js",
  "files": [
    "src/*",
    "*.json",
    "*.js"
  ],
  "engines": {
    "node": ">=12.22.0"
  },
  "bugs": {
    "url": "https://github.com/FrontLabsOfficial/vue-agile/issues"
  },
  "keywords": [
    "vue",
    "component",
    "slider",
    "carousel",
    "slide"
  ],
  "license": "MIT",
  "repository": {
    "typetype": "git",
    "url": "git+https://github.com/FrontLabsOfficial/vue-agile.git"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^2.0.1",
    "@vue/compiler-sfc": "^3.2.27",
    "chalk": "^4.1.1",
    "enquirer": "^2.3.6",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-vue": "^8.3.0",
    "execa": "^5.0.0",
    "lint-staged": "^12.2.1",
    "minimist": "^1.2.5",
    "prettier": "^2.5.1",
    "pug": "^3.0.2",
    "sass": "^1.49.0",
    "semver": "^7.3.5",
    "vite": "^2.7.13",
    "vue": "^3.2.27",
    "yorkie": "^2.0.0"
  }
}
